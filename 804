class Solution {
public:
    int uniqueMorseRepresentations(vector<string>& words) {
        vector<string> morse = {".-","-...","-.-.","-..",".","..-.",
		"--.","....","..",".---","-.-",".-..","--","-.","---",".--.",
		"--.-",".-.","...","-","..-","...-",".--","-..-","-.--","--.."};
		unordered_set<string> s;
		for(string word : words){
			string str = "";
			for(char c : word){
				str += morse[c - 'a'];
			}
			s.insert(str);
		}
		return s.size();
    }
};
